msgid "User interrupted process"
msgstr "Proceso interrumpido por el usuario"

#: src/syspurpose/utils.py:59
msgid ""
"Cannot create directory {}\n"
"Are you root?"
msgstr ""
"No se puede crear un directorio {}\n"
"¿Es usted root?"

