            <property name="visible">True</property>
            <accessibility>
              <atkproperty name="AtkObject::accessible-name" translatable="yes">connectionStatusLabel</atkproperty>
            </accessibility>
          </widget>
          <packing>
            <property name="position">3</property>
          </packing>
        </child>
        <child internal-child="action_area">
